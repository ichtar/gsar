def register(params)
    @field = params['field']
end

def filter(event)
  o = event.get(@field)
  temp={}
  name=""
  o.each do |k|
    k.each do |j,l|
      if l.is_a? String
        name=l
      else
        temp[j]=l
      end
    end
    event.set(name,temp)
    event.tag(name)
  end
  event.remove(@field)
  return [event]
end
